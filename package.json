{
  "name": "lambda-typescript-webpack",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean && yarn run webpack",
    "clean": "rimraf dist/*",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint --fix \"src/**/*.ts*\"",
    "format": "prettier *.ts \"src/**/*.ts*\" --write",
    "webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --mode production"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.50",
    "@types/node": "^13.13.2",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "builtin-modules": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "json-loader": "^0.5.7",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.1",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "aws-sdk": "^2.658.0",
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
